###  http://purl.obolibrary.org/obo/BFO_0000176
<http://purl.obolibrary.org/obo/BFO_0000176> rdf:type owl:ObjectProperty .


###  http://purl.obolibrary.org/obo/BFO_0000178
<http://purl.obolibrary.org/obo/BFO_0000178> rdf:type owl:ObjectProperty .


###  http://www.github/argO/2016/9/argumentsontology/conclusionIn
:conclusionIn rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf <http://purl.obolibrary.org/obo/BFO_0000176> ;
              owl:inverseOf :hasConclusion ;
              rdfs:domain :sentencecontent ;
              rdfs:range :argument ;
              rdfs:label "conclusion in"@en .


###  http://www.github/argO/2016/9/argumentsontology/contradicts
:contradicts rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :opposes ;
             owl:inverseOf :isContradictedBy ;
             rdfs:label "contradicts"@en .


###  http://www.github/argO/2016/9/argumentsontology/creates
:creates rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf <https://www.commoncoreontologies.org/ont00001986> ;
         owl:inverseOf :iscreatedby ;
         rdfs:domain <http://purl.obolibrary.org/obo/BFO_0000015> ;
         rdfs:range <http://purl.obolibrary.org/obo/BFO_0000002> ;
         rdfs:label "creates"@en ;
         <http://www.w3.org/2004/02/skos/core#definition> "X creates Y iff Y is created by X."@en .


###  http://www.github/argO/2016/9/argumentsontology/hasConclusion
:hasConclusion rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf <http://purl.obolibrary.org/obo/BFO_0000178> ;
               rdfs:domain :argument ;
               rdfs:range :sentencecontent ;
               rdfs:label "has conclusion"@en .


###  http://www.github/argO/2016/9/argumentsontology/hasPremise
:hasPremise rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf <http://purl.obolibrary.org/obo/BFO_0000178> ;
            owl:inverseOf :premiseIn ;
            rdfs:domain :argument ;
            rdfs:range :sentencecontent ;
            rdfs:label "has premise"@en ;
            <http://www.w3.org/2004/02/skos/core#definition> "x has premise y iff x is an argument, y is a sentence content, z is an act of inferring, x has continaunt part y, y is affirmed, y is input of z, and z provides support or justification for the conclusion of x."@en .


###  http://www.github/argO/2016/9/argumentsontology/hasProperContinuantPart
:hasProperContinuantPart rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf <http://purl.obolibrary.org/obo/BFO_0000178> ;
                         owl:inverseOf :properContinuantPartOf ;
                         rdfs:domain <http://purl.obolibrary.org/obo/BFO_0000002> ;
                         rdfs:range <http://purl.obolibrary.org/obo/BFO_0000002> ;
                         test:teste8bb6cb2_117b_4bfe_b5a6_a95b6576a498 "X has proper continuant part Y where X and Y are both continuants and X is part of Y but X is not identical to Y."@en ;
                         rdfs:label "has proper continuant part"@en .


###  http://www.github/argO/2016/9/argumentsontology/hasSubconclusion
:hasSubconclusion rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf <http://purl.obolibrary.org/obo/BFO_0000178> ;
                  owl:inverseOf :subconclusionIn ;
                  rdfs:domain argumentsontology:complexargument ;
                  rdfs:range :sentencecontent ;
                  rdfs:label "has subconclusion"@en .


###  http://www.github/argO/2016/9/argumentsontology/hasSupposition
:hasSupposition rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf <http://purl.obolibrary.org/obo/BFO_0000178> ;
                owl:inverseOf :suppositionIn ;
                rdfs:domain :argument ;
                rdfs:range :sentencecontent ;
                rdfs:label "has supposition"@en ;
                <http://www.w3.org/2004/02/skos/core#definition> "x has supposition y iff x is an argument, y is a sentence content, z is an act of inferring, x has continuant part y,  y is accepted but not affirmed, y is input of z, z provides support or justification for the conclusion of x."@en .


###  http://www.github/argO/2016/9/argumentsontology/isContradictedBy
:isContradictedBy rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :isOpposedBy ;
                  rdfs:label "is contradicted by"@en .


###  http://www.github/argO/2016/9/argumentsontology/isNegatedBy
:isNegatedBy rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :isOpposedBy ;
             owl:inverseOf :negates ;
             rdfs:label "is negated by"@en .


###  http://www.github/argO/2016/9/argumentsontology/isOpposedBy
:isOpposedBy rdf:type owl:ObjectProperty ;
             owl:inverseOf :opposes ;
             rdfs:label "is opposed by"@en .


###  http://www.github/argO/2016/9/argumentsontology/isSupportedBy
:isSupportedBy rdf:type owl:ObjectProperty ;
               owl:inverseOf :supports ;
               rdfs:label "is supported by"@en .


###  http://www.github/argO/2016/9/argumentsontology/isUnderminedBy
:isUnderminedBy rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :isOpposedBy ;
                owl:inverseOf :undermines ;
                rdfs:label "is undermined by"@en .


###  http://www.github/argO/2016/9/argumentsontology/iscreatedby
:iscreatedby rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf <https://www.commoncoreontologies.org/ont00001816> ;
             rdf:type owl:AsymmetricProperty ,
                      owl:IrreflexiveProperty ;
             rdfs:domain <http://purl.obolibrary.org/obo/BFO_0000002> ;
             rdfs:range <http://purl.obolibrary.org/obo/BFO_0000015> ;
             test:teste8bb6cb2_117b_4bfe_b5a6_a95b6576a498 "X is created by Y iff X is output of Y and the terminal boundary of the temporal region (W) on which Y occurs interval_meets a terminal region (Z) at which X exists and X does not exist at W."@en ;
             <http://purl.obolibrary.org/obo/IAO_0000117> "J. Neil Otte"@en ;
             :example "An argument is created by some act of argument creation"@en ;
             rdfs:label "is created by"@en .


###  http://www.github/argO/2016/9/argumentsontology/negates
:negates rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :opposes ;
         rdfs:label "negates"@en .


###  http://www.github/argO/2016/9/argumentsontology/opposes
:opposes rdf:type owl:ObjectProperty ;
         rdfs:label "opposes"@en .


###  http://www.github/argO/2016/9/argumentsontology/premiseIn
:premiseIn rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf <http://purl.obolibrary.org/obo/BFO_0000176> ;
           rdfs:domain :sentencecontent ;
           rdfs:range :argument ;
           rdfs:label "premise in"@en ;
           <http://www.w3.org/2004/02/skos/core#definition> "x premise in y iff y has premise x."@en .


###  http://www.github/argO/2016/9/argumentsontology/properContinuantPartOf
:properContinuantPartOf rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf <http://purl.obolibrary.org/obo/BFO_0000176> ;
                        rdf:type owl:TransitiveProperty ;
                        rdfs:domain <http://purl.obolibrary.org/obo/BFO_0000002> ;
                        rdfs:range <http://purl.obolibrary.org/obo/BFO_0000002> ;
                        rdfs:label "proper continuant part of"@en ;
                        <http://www.w3.org/2004/02/skos/core#definition> "X proper continuant part of Y iff Y has proper continuant part X."@en .


###  http://www.github/argO/2016/9/argumentsontology/subconclusionIn
:subconclusionIn rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf <http://purl.obolibrary.org/obo/BFO_0000176> ;
                 rdfs:domain :sentencecontent ;
                 rdfs:range argumentsontology:complexargument ;
                 rdfs:label "subconclusion in"@en .


###  http://www.github/argO/2016/9/argumentsontology/supports
:supports rdf:type owl:ObjectProperty ;
          rdfs:label "supports"@en .


###  http://www.github/argO/2016/9/argumentsontology/suppositionIn
:suppositionIn rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf <http://purl.obolibrary.org/obo/BFO_0000176> ;
               rdfs:domain :sentencecontent ;
               rdfs:range :argument ;
               rdfs:label "supposition in"@en ;
               <http://www.w3.org/2004/02/skos/core#definition> "x supposition in y iff y has supposition x."@en .


###  http://www.github/argO/2016/9/argumentsontology/undermines
:undermines rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :opposes ;
            rdfs:label "undermines"@en .


###  https://www.commoncoreontologies.org/ont00001816
<https://www.commoncoreontologies.org/ont00001816> rdf:type owl:ObjectProperty .


###  https://www.commoncoreontologies.org/ont00001986
<https://www.commoncoreontologies.org/ont00001986> rdf:type owl:ObjectProperty .


#################################################################
#    Classes
#################################################################

###  http://purl.obolibrary.org/obo/BFO_0000002
<http://purl.obolibrary.org/obo/BFO_0000002> rdf:type owl:Class .


###  http://purl.obolibrary.org/obo/BFO_0000015
<http://purl.obolibrary.org/obo/BFO_0000015> rdf:type owl:Class .


###  http://purl.obolibrary.org/obo/BFO_0000031
<http://purl.obolibrary.org/obo/BFO_0000031> rdf:type owl:Class .


###  http://www.github/argO/2016/9/argumentsontology#complexargument
argumentsontology:complexargument rdf:type owl:Class ;
                                  rdfs:subClassOf :argument ,
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :hasProperContinuantPart ;
                                                    owl:someValuesFrom :argument
                                                  ] ;
                                  test:test6c58c70d_1fcf_4414_be44_85e5cba8571b "J. Neil Otte"@en ;
                                  rdfs:label "complex argument"@en ;
                                  <http://www.w3.org/2004/02/skos/core#definition> "An argument that has some argument as a proper continuant part, and thus also some subconclusion."@en .


###  http://www.github/argO/2016/9/argumentsontology/actofaccepting
:actofaccepting rdf:type owl:Class ;
                rdfs:subClassOf :languageact ;
                test:test6c58c70d_1fcf_4414_be44_85e5cba8571b "J. Neil Otte"@en ;
                rdfs:label "act of accepting"@en ;
                <http://www.w3.org/2004/02/skos/core#altLabel> "supposing"@en ;
                <http://www.w3.org/2004/02/skos/core#definition> "A language act in which one posits, imagines, pretends, entertains, or otherwise supposes some statement, typically for the purpose of using it to make an inference."@en .


###  http://www.github/argO/2016/9/argumentsontology/actofaffirming
:actofaffirming rdf:type owl:Class ;
                rdfs:subClassOf :languageact ;
                test:test6c58c70d_1fcf_4414_be44_85e5cba8571b "J. Neil Otte"@en ;
                rdfs:label "act of affirming"@en ;
                <http://www.w3.org/2004/02/skos/core#definition> "a language act in which a sentence content is stated implicitly or explicitly in a forceful or confident manner, which conveys to others that the agent of the language act believes or has confidence in the truth or accuracy of the sentence content."@en .


###  http://www.github/argO/2016/9/argumentsontology/actofarguing
:actofarguing rdf:type owl:Class ;
              rdfs:subClassOf :languageact ;
              test:test6c58c70d_1fcf_4414_be44_85e5cba8571b "J. Neil Otte"@en ;
              rdfs:comment "Note: \"Act of Arguing\" differs from \"Act of Argument Creation\" in two respects: first, one can argue for some conclusion without creating an argument (e.g. by merely reusing an available argument); secondly, one can create an argument in an act of argument creation without any intention of convincing others of the truth of the conclusion. Therefore, 'act of arguing' is synonymous with 'trying to convince someone on the basis of reasons', whereas 'act of argument creation' explicitly involves coming up with an argument."@en ;
              rdfs:label "act of arguing"@en ;
              <http://www.w3.org/2004/02/skos/core#definition> "A language act in which one creates an argument or uses an argument with the purpose of convincing others that the conclusion of the argument is true or compelling."@en .


###  http://www.github/argO/2016/9/argumentsontology/actofargumentcreation
:actofargumentcreation rdf:type owl:Class ;
                       rdfs:subClassOf :languageact ;
                       test:test6c58c70d_1fcf_4414_be44_85e5cba8571b "J. Neil Otte"@en ;
                       rdfs:label "act of argument creation"@en ;
                       <http://www.w3.org/2004/02/skos/core#definition> "A language act in which an agent creates an argument by combining one or more premises or suppositions to support a conclusion."@en .


###  http://www.github/argO/2016/9/argumentsontology/actofinferring
:actofinferring rdf:type owl:Class ;
                rdfs:subClassOf :languageact ;
                <http://purl.obolibrary.org/obo/IAO_0000117> "Yonatan Schrieber"@en ;
                rdfs:comment "Note: Act of Inferring should be defined as something like \"A language act in which an agent indicates a relation between sentence contents such that some set of sentence contents provide reasons for holding another sentence content, as entailment, support, or justification.\""@en ;
                rdfs:label "act of inferring"@en ;
                <http://www.w3.org/2004/02/skos/core#altLabel> "inferring"@en ;
                <http://www.w3.org/2004/02/skos/core#definition> "A language act in which an agent uses one or more sentence contents to support, justify, entail, or assert the truth of another statement."@en .


###  http://www.github/argO/2016/9/argumentsontology/argument
:argument rdf:type owl:Class ;
          rdfs:subClassOf :collectionofsentencecontents ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :iscreatedby ;
                            owl:someValuesFrom :actofargumentcreation
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasConclusion ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass :sentencecontent
                          ] ;
          test:test6c58c70d_1fcf_4414_be44_85e5cba8571b "J. Neil Otte"@en ;
          rdfs:comment "Note: A single sentence content may form an argument, but it would need to bear both a 'has premise' and 'has conclusion' relation to the argument."@en ;
          rdfs:label "argument"@en ;
          <http://www.w3.org/2004/02/skos/core#definition> "An ordered collection of sentence contents that consists of at least one premise or supposition, and possibly one or more subconclusions, where from these parts exactly one conclusion is inferred, supported, or justified."@en ;
          <http://www.w3.org/2004/02/skos/core#example> "All men are mortal. Socrates is a man. Therefore, Socrates is mortal."@en .


###  http://www.github/argO/2016/9/argumentsontology/collectionofsentencecontents
:collectionofsentencecontents rdf:type owl:Class ;
                              rdfs:subClassOf <https://www.commoncoreontologies.org/ont00000958> ;
                              test:test6c58c70d_1fcf_4414_be44_85e5cba8571b "J. Neil Otte"@en ;
                              rdfs:comment "Note: Collections are distinct from the related notions of sets and aggregates. Unlike sets, which can not have the same member twice, a collection can have the same item more than once. Also, unlike aggregates (see BFO Reference 2.0), for any collection C, if b if an item of C, then it is not a restriction on C  there exists_at t a mutually exhaustive and pairwise disjoint partition of C into continuants x1,  …, xn (for some n > 1) with b = xi for some 1 ≤ i ≤ n."@en ;
                              rdfs:label "collection of sentence contents"@en ;
                              <http://www.w3.org/2004/02/skos/core#definition> "An information content entity that consists of two or more sentence contents."@en .


###  http://www.github/argO/2016/9/argumentsontology/conclusion
:conclusion rdf:type owl:Class ;
            rdfs:subClassOf :sentencecontent ;
            rdfs:label "conclusion"@en ;
            <http://www.w3.org/2004/02/skos/core#definition> "A sentence content that is an affirmed or accepted output of an act of inferring, thought to be supported or justified by corresponding affirmed or accepted inputs, and is conclusion in an argument."@en .


###  http://www.github/argO/2016/9/argumentsontology/expression
:expression rdf:type owl:Class ;
            rdfs:subClassOf <https://www.commoncoreontologies.org/ont00000314> ;
            rdfs:label "expression"@en ;
            <http://www.w3.org/2004/02/skos/core#definition> "An information quality entity that consists in patterns of character shapes given conventional linguistic meaning."@en .


###  http://www.github/argO/2016/9/argumentsontology/languageact
:languageact rdf:type owl:Class ;
             rdfs:subClassOf <https://www.commoncoreontologies.org/ont00000228> ;
             test:test6c58c70d_1fcf_4414_be44_85e5cba8571b "Yonatan Schrieber"@en ;
             rdfs:comment "Note: Language acts include speech acts and document acts, and are a subclass of the broader class of social acts (see Reinach on social acts)"@en ;
             rdfs:label "language act"@en ;
             <http://www.w3.org/2004/02/skos/core#definition> "A planned act undertaken by an agent through the use of a language."@en .


###  http://www.github/argO/2016/9/argumentsontology/premise
:premise rdf:type owl:Class ;
         rdfs:subClassOf :sentencecontent ;
         rdfs:label "premise"@en ;
         <http://www.w3.org/2004/02/skos/core#definition> "A sentence content that is affirmed and is input to an act of inferring that provides support or justification for the conclusion of an argument."@en .


###  http://www.github/argO/2016/9/argumentsontology/sentence
:sentence rdf:type owl:Class ;
          rdfs:subClassOf :expression ;
          rdfs:label "sentence"@en ;
          <http://www.w3.org/2004/02/skos/core#definition> "An expression that must be usable on its own to express content."@en .


###  http://www.github/argO/2016/9/argumentsontology/sentencecontent
:sentencecontent rdf:type owl:Class ;
                 rdfs:subClassOf <https://www.commoncoreontologies.org/ont00000958> ;
                 test:test6c58c70d_1fcf_4414_be44_85e5cba8571b "J. Neil Otte"@en ;
                 rdfs:comment "Editor's note: Logicians usually speak of 'propositions' as reasons, premises, conclusions, and the like. For this purpose, we use the term 'statement' for reasons explained in 'ArgO: an arguments ontology 2018'. However, in an effort to remain open to other logics that have argument parts that do not consist solely of propositions, we use the term 'sentence content' as our broader category, which is designed also to represent the content of meaningful sentences that do not express propositions, such as imperatives and interrogatives. The definition is also designed to leave open whether or not a sentence content is conveyed in a form other than a sentence in a language, for instance, a stop sign."@en ;
                 rdfs:label "sentence content"@en ;
                 <http://www.w3.org/2004/02/skos/core#definition> "An information content entity that is the content or meaning typically expressed in an atomic sentence in a language."@en ;
                 <http://www.w3.org/2004/02/skos/core#example> "\"2+2 =5\""@en ,
                                                               "\"Close the door!\""@en ,
                                                               "\"Is the present King of France bald?\""@en ,
                                                               "\"The man walked into the bar.\""@en .


###  http://www.github/argO/2016/9/argumentsontology/statement
:statement rdf:type owl:Class ;
           rdfs:subClassOf :sentencecontent ;
           test:test6c58c70d_1fcf_4414_be44_85e5cba8571b "J. Neil Otte"@en ;
           rdfs:comment "Note: 'Statement' is similar to what logicians often call 'propositions'; however, it is important to point out that, like all generically dependent continuants (GDCs) in Basic Formal Ontology, it is given quite a distinct account of grounding, which distinguishes it from common conceptions of propositions as Platonic, \"abstract\", or ungrounded entities. Statements, by contract, like all GDCs, will exist only so long as there is at least one bearer of them in existence."@en ;
           rdfs:label "statement"@en ;
           <http://www.w3.org/2004/02/skos/core#definition> "A sentence content that is the meaning of a declarative sentence in a language."@en ;
           <http://www.w3.org/2004/02/skos/core#example> "\"All penguins can swim.\""@en ,
                                                         "\"The present king of France is bald.\""@en .


###  http://www.github/argO/2016/9/argumentsontology/subconclusion
:subconclusion rdf:type owl:Class ;
               rdfs:subClassOf :sentencecontent ;
               rdfs:label "subconclusion"@en ;
               <http://www.w3.org/2004/02/skos/core#definition> "A sentence content that bears a subconclusion in relation to a complex argument."@en .


###  http://www.github/argO/2016/9/argumentsontology/supposition
:supposition rdf:type owl:Class ;
             rdfs:subClassOf :sentencecontent ;
             rdfs:label "supposition"@en ;
